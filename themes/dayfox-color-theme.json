  "tokenColors": [
    {
      "name": "Flow control, built-ins - this, super",
      "scope": [
        "keyword.control.flow",
        "keyword.control.trycatch",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#a5222f"
      }
    },
    {
      "name": "Class definitions, other keywords, tags",
      "scope": [
        "entity.name.tag",
        "storage.type",
        "storage.modifier",
        "keyword"
      ],
      "settings": {
        "foreground": "#6e33ce"
      }
    },
    {
      "name": "Class names, custom types",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class",
        "support.type.class",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#ac5402"
      }
    },
    {
      "name": "Function definitions, object members, class methods, attributes",
      "scope": [
        "storage.type.function",
        "variable.other.property",
        "variable.other.object.property",
        "entity.other.attribute-name",
        "support.type.property-name",
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#2848a9"
      }
    },
    {
      "name": "Function names",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#223d90"
      }
    },
    {
      "name": "Primitive types, parameters, components, CSS units",
      "scope": [
        "support.type.primitive",
        "support.type.builtin.primitive",
        "support.class.component",
        "variable.parameter",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#22676d"
      }
    },
    {
      "name": "Constants, decorators",
      "scope": ["constant", "meta.function.decorator"],
      "settings": {
        "foreground": "#7f5152"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#396847"
      }
    },
    {
      "name": "Punctuation, braces, parens",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": {
        "foreground": "#643f61"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#837a72"
      }
    },
    {
      "name": "Markdown: headings",
      "scope": ["markup.heading", "punctuation.definition.heading.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#223d90"
      }
    },
    {
      "name": "Markdown: link label",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6e33ce"
      }
    },
    {
      "name": "Markdown: link/image text",
      "scope": ["string.other.link.description.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2848a9"
      }
    },
    {
      "name": "Markdown: link URL",
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7f5152"
      }
    },
    {
      "name": "Markdown: bold",
      "scope": ["markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b3434e"
      }
    },
    {
      "name": "Markdown: italic",
      "scope": ["markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b3434e"
      }
    },
    {
      "name": "Markdown: strikethrough",
      "scope": ["markup.strikethrough.markdown"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown: inline string",
      "scope": ["markup.inline.raw.string.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#287980"
      }
    }
  ]
}
